{
  "name": "@bitnami/hex-react",
  "version": "4.0.0",
  "description": "React component library based on HEx design system",
  "main": "lib/hex.react.min.js",
  "files": [
    "lib",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "start": "webpack --watch --config webpack.dev.js --progress",
    "build:dev": "webpack --progress --config webpack.dev.js",
    "build": "webpack --progress --config webpack.prod.js",
    "prepublish": "yarn run build",
    "prepack": "yarn run build",
    "lint": "prettier-eslint --write 'src/**/*.{js,jsx,css,scss}'",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "test:integration": "node ./scripts/integration.js",
    "test:integration:single": "jest --ci --setupFilesAfterEnv ./setupIntegrationTests.js --testEnvironment ./integrationEnvironment.js --transform '{ \"^.*\\\\.js$\": \"<rootDir>/jestPreprocess.js\" }' --no-cache --testMatch **/__integration__/*.js ./__integration__/*.js",
    "test:a11y": "node ./scripts/pa11y.js"
  },
  "keywords": [
    "Bitnami",
    "User",
    "Interface",
    "Components",
    "Foundations",
    "Library"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,css,scss}": [
      "yarn lint",
      "git add"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Bitnami (https://design.bitnami.com)",
  "license": "Apache-2.0",
  "dependencies": {
    "@bitnami/hex": "^4.0.0",
    "classnames": "^2.2.6",
    "detect-indent": "^6.0.0",
    "lowlight": "^1.12.0",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "browserstack-local": "^1.4.0",
    "chalk": "^2.4.2",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.13.0",
    "highlight.js": "^10.4.1",
    "husky": "^2.2.0",
    "jest": "^24.7.1",
    "jest-environment-node": "^24.7.1",
    "jest-enzyme": "^7.0.2",
    "lint-staged": "^8.1.6",
    "listr": "^0.14.3",
    "npm-run-all": "^4.1.5",
    "pa11y": "^5.1.0",
    "pa11y-reporter-cli": "^1.0.1",
    "prettier": "^1.17.0",
    "prettier-eslint-cli": "^4.7.1",
    "puppeteer": "^1.15.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "wdio-browserstack-service": "^0.1.18",
    "webdriverio": "^5.8.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "peerDependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./setupTests.js"
    ]
  }
}
