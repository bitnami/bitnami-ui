{
  "name": "@bitnami/hex-react",
  "version": "3.2.0",
  "description": "React component library based on HEx design system",
  "main": "lib/hex.react.js",
  "scripts": {
    "start": "webpack --watch --config webpack.dev.js --progress",
    "build:dev": "webpack --progress --config webpack.dev.js",
    "build": "webpack --progress --config webpack.prod.js",
    "prepublish": "yarn run build",
    "prepack": "yarn run build",
    "lint": "prettier-eslint --write 'src/**/*.{js,jsx,css,scss}'",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "test:a11y": "node ./scripts/pa11y.js"
  },
  "keywords": [
    "Bitnami",
    "User",
    "Interface",
    "Components",
    "Foundations",
    "Library"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,css,scss}": [
      "yarn lint",
      "git add"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Bitnami (https://design.bitnami.com)",
  "license": "Apache-2.0",
  "dependencies": {
    "@bitnami/hex": "^3.2.0",
    "classnames": "^2.2.6",
    "prop-types": "^15.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.2",
    "chalk": "^2.4.1",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.1",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.0.0-rc.13",
    "jest": "^23.5.0",
    "jest-enzyme": "^6.0.4",
    "lint-staged": "^7.2.2",
    "npm-run-all": "^4.1.3",
    "pa11y": "^5.0.4",
    "pa11y-reporter-cli": "^1.0.1",
    "prettier": "^1.14.2",
    "prettier-eslint-cli": "^4.7.1",
    "puppeteer": "^1.7.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7",
    "webpack-merge": "^4.1.4"
  },
  "peerDependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./setupTests.js"
  }
}
